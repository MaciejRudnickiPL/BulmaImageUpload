// pomocnicza ogólna klasa dla js

function HelpJS() {
    this.infoLevel = 10;
}

// krótki sygnał dzwiękowy beep
HelpJS.prototype.beep = function () {
    let snd = new Audio("data:audio/wav;base64,");
    snd.play();
};
//zmiana przecinak na kropkę
// przy rozpoznawaniu liczby
HelpJS.prototype.changeToDot = function (value) {
    return value.replace(".", ",");
};
// wyświetla wszystke id na stroneie
HelpJS.prototype.showId = function () {
    let idList = document.querySelectorAll('[id]');

    for (let i = 0, len = idList.length; i < len; i++) {
        let idElement = idList[i];
        let poz = idElement.offsetTop + 30;
        let info = '<div style="top:' + poz + 'px;left:0 " class="helpToolsJsIdBox">' + idList[i].id + '</div>';
        idElement.insertAdjacentHTML('beforebegin', info);
    }

};
// wyświetla w konsoli listę id na stronie
HelpJS.prototype.showIdList = function () {
    let idList = document.querySelectorAll('[id]');
    console.log('-----------ID LISTA ----------------------');
    for (let i = 0, len = idList.length; i < len; i++) {
        console.log(i + ': ' + idList[i].id);
    }
    console.log('---------------------------------');
};
HelpJS.prototype.get_browser_info = function () {

    let ua = navigator.userAgent, tem,
        M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
    if (/trident/i.test(M[1])) {
        tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
        return {name: 'IE ', version: (tem[1] || '')};
    }
    if (M[1] === 'Chrome') {
        tem = ua.match(/\bOPR\/(\d+)/);
        if (tem != null) {
            return {name: 'Opera', version: tem[1]};
        }
    }
    M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
    if ((tem = ua.match(/version\/(\d+)/i)) != null) {
        M.splice(1, 1, tem[1]);
    }
    return {
        name: M[0],
        version: M[1]
    };

};
HelpJS.prototype.info = function (info, level = 0) {
    // console.log('infoLEvel: ' + this.infoLevel + '<' + level);
    // dodanie jak drugi paramet jakiegoś znaku nie liczbowego
    // powoduje wydruk odkreślenia tym znakiem
    if (isNaN(level)) {
        if (level.length <= 3) {
            for (let i = 0; i < 5; i++) {
                level = level + level;
            }
            console.log(level);
            console.log(info);

            console.log(level);
        } else {
            let przerwa = ' ******** ';
            console.log(przerwa + level + przerwa);
            console.log(info);
            console.log('---------------------------');
        }


    } else {
        if (this.infoLevel > level) {
            console.log(info);
        }
    }
};
//generuje nazwe z losową konówką numeryczną quantity określa ilość (tyle co zer)
HelpJS.prototype.getRendomName = function (strName, quantity = 1000000) {
    return strName + Math.floor((Math.random() * quantity) + 1);
};

HelpJS.prototype.randomText = function () {
    return Math.random().toString(36).substr(2);
};
//sprawdzanie czy plik zmienne jest pusta
HelpJS.prototype.isEmpty = function (str) {
    return typeof str == 'undefined' || !str || str.length === 0 || str === "" || !/[^\s]/.test(str) || /^\s*$/.test(str) || str.replace(/\s/g, "") === "";
};
//sprawdza czy zeminna to obiekt czy
HelpJS.prototype.isObject = function (value) {
    return value && typeof value === 'object' && value.constructor === Object;
};

HelpJS.prototype.arrayRemove = function (myArray, objectName) {

};

export default HelpJS;
